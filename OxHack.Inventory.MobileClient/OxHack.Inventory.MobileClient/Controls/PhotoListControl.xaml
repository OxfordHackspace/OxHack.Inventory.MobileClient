	<StackLayout Orientation="Horizontal">
		<StackLayout x:Name="photoLayout" Orientation="Horizontal">
		</StackLayout>
		<Button FontSize="48" WidthRequest="100" VerticalOptions="FillAndExpand" Text="+"/>
	</StackLayout>

</ScrollView>