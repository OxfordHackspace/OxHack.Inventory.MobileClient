						xmlns:controls="clr-namespace:OxHack.Inventory.MobileClient.Controls"
						x:Class="OxHack.Inventory.MobileClient.Controls.PhotoListControl"
						Orientation="Horizontal"
						HorizontalOptions="CenterAndExpand"
